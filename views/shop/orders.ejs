<%- include('../includes/head.ejs')%>
<body>
    <%- include('../includes/navigation.ejs')%>


    <main>
        <% if(orders.length<=0){ %>
            <h1>nothing</h1>
        <% } else { %>
            <ul>
                <% orders.forEach(order=>{%>
                    <li>
                        <h1>order #<%= order._id %></h1>
                        <ul>
                            <% order.products.forEach(product =>{ %>
                                <li><%= product.productData %> (<%= product.quantity %>)</li>
                            <% }) %>
                        </ul>
                    </li>
                <% }) %>
            </ul>
            <% } %>
    </main>

    <%- include('../includes/end.ejs')%>
